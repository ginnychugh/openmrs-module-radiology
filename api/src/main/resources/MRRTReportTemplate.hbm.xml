<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.radiology">
	<class name="org.openmrs.module.radiology.report.template.MRRTReportTemplate"
		table="radiology_report_template" lazy="false">
		<id name="templateId" column="template_id">
			<generator class="native">
				<param name="sequence">radiology_report_template_template_id_seq</param>
			</generator>
		</id>
		
		<property name="charset" type="java.lang.String" column="charset"/>
		<property name="path" type="java.lang.String" column="path"/>
		<property name="title" type="java.lang.String" column="dcterms_title"/>
		<property name="description" type="java.lang.String" column="dcterms_description"/>
		<property name="identifier" type="java.lang.String" column="dcterms_identifier"/>
		<property name="type" type="java.lang.String" column="dcterms_type"/>
		<property name="language" type="java.lang.String" column="dcterms_langauge"/>
		<property name="publisher" type="java.lang.String" column="dcterms_publisher"/>
		<property name="rights" type="java.lang.String" column="dcterms_rights"/>
		<property name="license" type="java.lang.String" column="dcterms_lisence"/>
		<property name="date" type="java.lang.String" column="dcterms_date"/>
		<property name="creator" type="java.lang.String" column="dcterms_creator"/>
	</class>
</hibernate-mapping>