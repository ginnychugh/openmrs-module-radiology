<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="org.openmrs.module.radiology">
	<class name="org.openmrs.module.radiology.report.template.MrrtReportTemplate"
		table="radiology_report_template" lazy="false">
		<id name="templateId" column="template_id">
			<generator class="native">
				<param name="sequence">radiology_report_template_template_id_seq</param>
			</generator>
		</id>
		
		<property name="charset" type="java.lang.String"/>
		<property name="path" type="java.lang.String"/>
		<property name="title" column="dcterms_title" type="java.lang.String" />
		<property name="description" column="dcterms_description" type="java.lang.String"/>
		<property name="identifier" column="dcterms_identifier" type="java.lang.String"/>
		<property name="type" column="dcterms_type" type="java.lang.String"/>
		<property name="language" column="dcterms_language" type="java.lang.String"/>
		<property name="publisher" column="dcterms_publisher" type="java.lang.String"/>
		<property name="rights" column="dcterms_rights" type="java.lang.String"/>
		<property name="license" column="dcterms_license" type="java.lang.String"/>
		<property name="date" column="dcterms_date" type="java.lang.String"/>
		<property name="creator" column="dcterms_creator" type="java.lang.String"/>
	</class>
</hibernate-mapping>